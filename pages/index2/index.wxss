page{ 
  display: flex; 
  flex-direction: column; 
  height: 100%; 
} 
.navbar{ 
  flex: none; 
  display: flex; 
  background: #fff; 
} 
.navbar .item{ 
  position: relative; 
  flex: auto; 
  text-align: center; 
  line-height: 80rpx; 
  font-size:17px;
  font-family: 微软雅黑;
  font-weight: lighter;
  color: #666666;
} 
/* 顶部导航字体颜色 */
.navbar .item.active{ 
  color: #11AA78; 
} 

/* 顶部指示条属性 */

.navbar .item.active:after{ 
  content: ""; 
  display: block; 
  position: absolute; 
  bottom: 0; 
  left: 0; 
  right: 0; 
  height: 6rpx; 
  background: #11AA78; 
}
.weui-search-bar {
  position: relative;
  padding: 16rpx 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  
  display: block;
  box-sizing: border-box;
  background-color: #f5f5f5;
  border-top: 1rpx solid #F0F0F0;
  border-bottom: 1rpx solid #F0F0F0;
}
.weui-icon-search {
  margin-right: 16rpx;
  font-size: inherit;
}

.weui-icon-search_in-box {
  position: absolute;
  left: 20rpx;
  top: 14rpx;
}
.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 5px;
  background: #F0F0F0;
  border: 1rpx solid #F0F0F0;
}
.weui-search-bar__box {
  position: relative;
  padding-left: 60rpx;
  padding-right: 60rpx;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
  background-color: #fff;
  border-radius: 10rpx;
}

.weui-search-bar__input {
  height: 62rpx;
  line-height: 62rpx;
  font-size: 28rpx;
  background-color: #fff;
}

.weui-icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 8px;
  font-size: 0;
}
.weui-search-bar__label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 6rpx;
  text-align: center;
  color: #D8D8D8 ;
  background: #fff;
  line-height: 56rpx;
}
.weui-search-bar__cancel-btn {
  margin-left: 20rpx;
  line-height: 56rpx;
  color:#666666;
  white-space: nowrap;
}
